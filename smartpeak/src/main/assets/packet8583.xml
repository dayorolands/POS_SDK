        <field id="88" type="CHAR" length="16" />
        <field id="89" type="CHAR" length="16" />
        <field id="90" type="CHAR" length="10" />
        <field id="96" type="LLLBINARY" />
        <field id="97" type="CHAR" length="17"/>
        <field id="98" type="CHAR" length="25" />
        <field id="100" type="LLVAR" />
        <field id="101" type="LLVAR" />
        <field id="102" type="LLVAR" />
        <field id="103" type="LLVAR" />
        <field id="104" type="LLLVAR" />
