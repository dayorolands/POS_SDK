        <field id="29" type="CHAR" length="3" />
        <field id="30" type="CHAR" length="24" />      
        <field id="32" type="LLVAR" />
        <field id="35" type="LLVAR" align="left" />
        <field id="37" type="CHAR" length="12" />
        <field id="38" type="CHAR" length="6" />
        <field id="39" type="CHAR" length="3" />
        <field id="40" type="CHAR" length="3" />
        <field id="41" type="CHAR" length="8" />
        <field id="42" type="CHAR" length="15" />
        <field id="44" type="LLVAR" />
        <field id="45" type="LLVAR" />
        <field id="46" type="LLLVAR" />
        <field id="47" type="LLLVAR" />
        <field id="48" type="BITMAPCHILD" length="2">
        	<child id="1" type="CHAR" length="19" />
        	<child id="2" type="CHAR" length="2" />
        	<child id="3" type="CHAR" length="4" />
        	<child id="4" type="CHAR" length="4"  />
        	<child id="5" type="LLLVAR" />
        	<child id="6" type="LLLVAR" />
        	<child id="7" type="CHAR" length="48"/>
        	<child id="8" type="LLVAR" />
        	<child id="9" type="CHAR" length="29" />
        	<child id="10" type="CHAR" length="1" />
        	<child id="11" type="CHAR" length="1" />
        	<child id="12" type="LLLVAR" />
        	<child id="13" type="CHAR" length="31" />
        	<child id="14" type="CHAR" length="253" />
        	<child id="15" type="LLVAR" />
        	<child id="16" type="LLLVAR" />
        </field>
        <field id="49" type="CHAR" length="3" />
        <field id="50" type="CHAR" length="3" />
        <field id="52" type="BINARY" length="8" />
        <field id="53" type="LLBINARY"/>
        <field id="54" type="LLLVAR" />
        <field id="55" type="LLLBINARY" />
        <field id="56" type="LLVAR" />
        <field id="57" type="CHAR" length="3"/>
        <field id="58" type="LLVAR" />
        <field id="59" type="LLLVAR" />      
        <field id="62" type="LLLVAR" />
