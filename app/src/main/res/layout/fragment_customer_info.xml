<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/constraintLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="@dimen/activity_horizontal_margin"
    tools:context="com.appzonegroup.app.fasttrack.WithdrawalByAPITokenActivity$CustomerInfoFragment">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <com.appzonegroup.app.fasttrack.ui.EditText
            android:id="@+id/account_number_et"
            style="@style/defaultEditText"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="10dp"
            android:hint="@string/phoneNumber_hint"
            android:inputType="number"
            android:maxLength="10" />


        <com.appzonegroup.app.fasttrack.ui.EditText
            android:id="@+id/agent_pin_et"
            style="@style/defaultEditText"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="10dp"
            android:hint="@string/agentPIN_hint"
            android:inputType="numberPassword"
            android:maxLength="10" />

        <com.appzonegroup.app.fasttrack.ui.CustomButton
            android:id="@+id/send_token_btn"
            style="@style/button_style"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="send_token_clicked"
            android:text="@string/send_token_to_customer" />


    </LinearLayout>


</androidx.constraintlayout.widget.ConstraintLayout>